entity DamageEntity {
    damageName String
    damageCode String
    damageFamily String
    damageGenus String
    damageSpecies String
    createdAt LocalDate
    addedBy String
}

entity DamageCategory{
  categoryName String
}

entity DamageType{
  typeName String
}

/**
 * Many to one relationship.
 */
relationship ManyToOne {
  DamageEntity{damageCategory} to DamageCategory
  DamageEntity{damageType} to DamageType
}
service * with serviceClass
paginate * with pagination
filter *